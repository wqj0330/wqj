   <!-- UNION 指令的目的是将两个 SQL 语句的结果合并起来  LEFT JOIN 左连接-->
   <!-- id="selectWithMany" 菜单权限设置  mybatis的传入参数parameterType   Login-->
  <select id="selectWithMany" parameterType="me.anchora.inpaasmgr.entry.inpaasmgr.Login" resultMap="WithManyResultMap">
    SELECT id, menu_name, parent_menu_id, menu_order, style_class, menu_item_id, menu_value FROM (
    SELECT m.id AS id, m.menu_name AS menu_name, m.parent_menu_id AS parent_menu_id, m.menu_order AS menu_order, m.style_class AS style_class, m.menu_item_id AS menu_item_id, md.menu_value AS menu_value FROM menus m
    <!-- left join(左联接) 返回包括左表中的所有记录和右表中联结字段相等的记录  -->
    <!-- LEFT JOIN实现表与表之间的关联 -->
    <!-- 左表menus,右表menusdetails -->
    LEFT JOIN menu_details md ON m.id = md.menu_id
    <!-- 左表menudetails,右表menuslanguage -->
    LEFT JOIN menu_languages ml ON md.lang_id = ml.id
    LEFT JOIN role_menus rm ON rm.menu_id = m.id
    LEFT JOIN role_users ru ON ru.role_id = rm.role_id
    LEFT JOIN login l ON l.id = ru.user_id
    <!-- WHERE条件 选择满足条件menu_languages的值为当前语言且login的user_name且role_menus的has_right='Y'-->
    WHERE 
    <!-- local对应home.jsp中 <fmt:setLocale value="${cookie.lang.value}" -->
    <!-- local为传入参数类型Login的属性，选择条件Login中local值与ml.lang_name相等 -->
    ml.lang_name = #{locale,jdbcType=VARCHAR} AND
    l.user_name = #{userName,jdbcType=VARCHAR} AND
    rm.has_right = 'Y'
    UNION
    SELECT m.id AS id, m.menu_name AS menu_name, m.parent_menu_id AS parent_menu_id, m.menu_order AS menu_order, m.style_class AS style_class, m.menu_item_id AS menu_item_id, md.menu_value AS menu_value FROM menus m
    LEFT JOIN menu_details md ON m.id = md.menu_id
    LEFT JOIN menu_languages ml ON md.lang_id = ml.id
    LEFT JOIN user_menus um ON um.menu_id = m.id
    LEFT JOIN login l ON l.id = um.user_id
    WHERE ml.lang_name = #{locale,jdbcType=VARCHAR} AND
    l.user_name = #{userName,jdbcType=VARCHAR} AND
    um.has_right = 'Y'
    ) t
    ORDER BY t.parent_menu_id, t.menu_order
  </select>
